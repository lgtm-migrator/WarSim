{
  "name": "warsim",
  "displayName": "War Sim",
  "version": "1.0.0",
  "description": "War Simulation game",
  "main": "index.js",
  "scripts": {
    "start": "./node_modules/.bin/tsc --build tsconfig.json --watch ",
    "build": "./node_modules/.bin/tsc --build tsconfig.json && npm run copy",
    "copy": "copyfiles -u 1 \"./src/**/*.{css,html}\" \"./www/\""
  },
  "keywords": [
    "ecosystem:cordova"
  ],
  "author": "Michael Milord",
  "license": "MIT",
  "dependencies": {
    "cordova-browser": "^6.0.0",
    "cordova-plugin-whitelist": "1"
  },
  "devDependencies": {
    "copyfiles": "^2.1.1",
    "cordova-plugin-whitelist": "^1.3.4",
    "parcel-bundler": "^1.12.3",
    "typescript": "^3.5.3"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {}
    },
    "platforms": [
      "browser"
    ]
  }
}